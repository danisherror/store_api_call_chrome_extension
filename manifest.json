{
  "name": "Api Call logger",
  "description": "Logs API calls similar to chrome DevTools Network tab",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html"
  },
  "permissions":[
    "webRequest",
    "storage"
  ],
   "host_permissions": [
    "<all_urls>"
  ],
  "background":{
    "service_worker":"background.js"
  }
}
  
